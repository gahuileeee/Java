public class ForTest {

	public static void main(String[] args) {
		
//		for
		for(/*초기식 ; 조건식 ; 증감식*/ 
			int i=1; i<=5 ; i++) {
			
		}
		System.out.println();
//		1부터 10까지 합
		
//		1부터 10까지 짝수합
		
//		for 중첩
		
//		구구단
		
//		별삼각형
		
	}

}
